algoritmo "semnome"


var
CONT,N,S , MAIOR :INTEIRO

inicio
CONT<- 1
S<- 1
ENQUANTO (CONT<=10) FACA
ESCREVA("DIGITE O " ,CONT, "O. NUMERO: ")
LEIA(N)
SE (N> MAIOR) ENTAO
MAIOR<- N
FIMSE
S<- S + N
CONT <- CONT + 1
FIMENQUANTO
ESCREVAL (" A SOMA DE TODOS OS VALORES FOI :", S)
ESCREVAL(" O MAIOR NUMERO DIGITADO FOI:" , MAIOR)
fimalgoritmo
